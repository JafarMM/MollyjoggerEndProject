
@{
    ViewData["Title"] = "Delete";
    SliderImages sliderImages = Model;
}

<div class="row">
    <div class="col-md-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <img src="~/Images/@sliderImages.Image" style="width:200px !important; height:200px !important;" />
                <p class="text-danger">Do you want to delete this category?</p>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <a asp-action="Index" class="btn btn-primary">Go to Back</a>
    <form method="post">
        <button type="submit" class="btn btn-danger mr-2">Delete</button>
    </form>
</div>