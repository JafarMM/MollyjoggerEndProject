 

<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Majestic Admin</title>
    <!-- plugins:css -->
    <link rel="stylesheet" href="~/AdminPanel/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/AdminPanel/base/vendor.bundle.base.css">
    <!-- endinject -->
    <!-- plugin css for this page -->
    <link rel="stylesheet" href="~/AdminPanel/datatables.net-bs4/dataTables.bootstrap4.css">
    <!-- End plugin css for this page -->
    <!-- inject:css -->
    <link rel="stylesheet" asp-append-version="true" href="~/AdminPanel/css/style.css">
    <!-- endinject -->
    <link rel="shortcut icon" href="~/AdminPanel/images/favicon.png" />
</head>
<body>
    <div class="container-scroller">
        <!-- partial:partials/_navbar.html -->
        <nav class="navbar col-lg-12 col-12 p-0 fixed-top d-flex flex-row">
            <div class="navbar-brand-wrapper d-flex justify-content-center">
                <div class="navbar-brand-inner-wrapper d-flex justify-content-between align-items-center w-100">
                    <a class="navbar-brand brand-logo" href="index.html"><img src="~/AdminPanel/images/logo.png" alt="logo" /></a>
                    <a class="navbar-brand brand-logo-mini" href="index.html"><img src="~/AdminPanel/images/logo-mini.svg" alt="logo" style="" /></a>
                    <button class="navbar-toggler navbar-toggler align-self-center" type="button" data-toggle="minimize">
                        <span class="mdi mdi-sort-variant"></span>
                    </button>
                </div>
            </div>
            <div class="navbar-menu-wrapper d-flex align-items-center justify-content-end">
                <ul class="navbar-nav mr-lg-4 w-100">
                    <li class="nav-item nav-search d-none d-lg-block w-100">
                        <div class="input-group">
                            <div class="input-group-prepend">
                                <span class="input-group-text" id="search">
                                    <i class="mdi mdi-magnify"></i>
                                </span>
                            </div>
                            <input type="text" class="form-control" placeholder="Search now" aria-label="search" aria-describedby="search">
                        </div>
                    </li>
                </ul>
                <ul class="navbar-nav navbar-nav-right">
                    <li class="nav-item nav-profile dropdown">
                        <div class="dropdown-menu dropdown-menu-right navbar-dropdown" aria-labelledby="profileDropdown">
                            <a class="dropdown-item">
                                <i class="mdi mdi-settings text-primary"></i>
                                Settings
                            </a>
                            <a class="dropdown-item">
                                <i class="mdi mdi-logout text-primary"></i>
                                Logout
                            </a>
                        </div>
                    </li>
                </ul>
                <button class="navbar-toggler navbar-toggler-right d-lg-none align-self-center" type="button" data-toggle="offcanvas">
                    <span class="mdi mdi-menu"></span>
                </button>
            </div>
        </nav>
        <!-- partial -->
        <div class="container-fluid page-body-wrapper">
            <!-- partial:partials/_sidebar.html -->
            <nav class="sidebar sidebar-offcanvas" id="sidebar">
                <ul class="nav">
                    <li class="nav-item">
                        <a class="nav-link" href="index.html">
                            <i class="mdi mdi-home menu-icon"></i>
                            <span class="menu-title">Dashboard</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" data-toggle="collapse" href="#ui-basic" aria-expanded="false" aria-controls="ui-basic">
                            <i class="mdi mdi-circle-outline menu-icon"></i>
                            <span class="menu-title">Categories</span>
                            <i class="menu-arrow"></i>
                        </a>
                        <div class="collapse" id="ui-basic">
                            <ul class="nav flex-column sub-menu">
                                <li class="nav-item"> <a class="nav-link" asp-area="AdminPanel" asp-controller="Category" asp-action="Index">Category List</a></li>
                                <li class="nav-item"> <a class="nav-link" asp-area="AdminPanel" asp-controller="Category" asp-action="Create">Add New Category</a></li>
                            </ul>
                        </div>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="AdminPanel" asp-controller="SliderImage" asp-action="Index">
                            <i class="mdi mdi-view-headline menu-icon"></i>
                            <span class="menu-title">Slider Images</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="AdminPanel" asp-controller="Product" asp-action="Index">
                            <i class="mdi mdi-chart-pie menu-icon"></i>
                            <span class="menu-title">Products</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="AdminPanel" asp-controller="AboutUs" asp-action="Index">
                            <i class="mdi mdi-grid-large menu-icon"></i>
                            <span class="menu-title">About Us</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="AdminPanel" asp-controller="User" asp-action="Index">
                            <i class="mdi mdi-emoticon menu-icon"></i>
                            <span class="menu-title">Users and Admins</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="AdminPanel" asp-controller="ReturnForm" asp-action="Index">
                            <i class="mdi mdi-emoticon menu-icon"></i>
                            <span class="menu-title">ReturnForms</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="AdminPanel" asp-controller="ShopPartReklam" asp-action="Index">
                            <i class="mdi mdi-emoticon menu-icon"></i>
                            <span class="menu-title">Shop Part Reklam</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="AdminPanel" asp-controller="KnifeKit" asp-action="Index">
                            <i class="mdi mdi-emoticon menu-icon"></i>
                            <span class="menu-title">FancyBox Video part</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="AdminPanel" asp-controller="Bloginhome" asp-action="Index">
                            <i class="mdi mdi-emoticon menu-icon"></i>
                            <span class="menu-title">Blog in Home</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="AdminPanel" asp-controller="Blog" asp-action="Index">
                            <i class="mdi mdi-emoticon menu-icon"></i>
                            <span class="menu-title">Blog</span>
                        </a>
                    </li>
                </ul>
            </nav>
            <!-- partial -->
            <div class="main-panel">
                <video autoplay muted loop id="myVideo">
                    <source src="~/AdminPanel/video/Every summer has its own story... Mollyjogger.com.mp4" type="video/mp4">
                </video>
                <div class="content-wrapper">
                    @RenderBody()

                </div>
                <!-- content-wrapper ends -->
                <!-- partial:partials/_footer.html -->
                <footer class="footer">
                   
                </footer>
                <!-- partial -->
            </div>
            <!-- main-panel ends -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>
    <!-- container-scroller -->
    <!-- plugins:js -->
    <script src="~/AdminPanel/vendors/base/vendor.bundle.base.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page-->
    <script src="~/AdminPanel/vendors/chart.js/Chart.min.js"></script>
    <script src="~/AdminPanel/vendors/datatables.net/jquery.dataTables.js"></script>
    <script src="~/AdminPanel/vendors/datatables.net-bs4/dataTables.bootstrap4.js"></script>
    <!-- End plugin js for this page-->
    <!-- inject:js -->
    <script src="~/AdminPanel/js/off-canvas.js"></script>
    <script src="~/AdminPanel/js/hoverable-collapse.js"></script>
    <script src="~/AdminPanel/js/template.js"></script>
    <!-- endinject -->
    <!-- Custom js for this page-->
    <script src="~/AdminPanel/js/dashboard.js"></script>
    <script src="~/AdminPanel/js/data-table.js"></script>
    <script src="~/AdminPanel/js/jquery.dataTables.js"></script>
    <script src="~/AdminPanel/js/dataTables.bootstrap4.js"></script>
    <!-- End custom js for this page-->
    <script src="~/AdminPanel/js/jquery.cookie.js" type="text/javascript"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.3/jquery.validate.min.js" integrity="sha512-37T7leoNS06R80c8Ulq7cdCDU5MNQBwlYoy1TX/WUsLFC2eYNqtKlV0QjH7r8JpG/S0GUMZwebnVFLPd6SU5yg==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validation-unobtrusive/3.2.12/jquery.validate.unobtrusive.min.js" integrity="sha512-o6XqxgrUsKmchwy9G5VRNWSSxTS4Urr4loO6/0hYdpWmFUfHqGzawGxeQGMDqYzxjY9sbktPbNlkIQJWagVZQg==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script src="https://cdn.ckeditor.com/ckeditor5/29.1.0/classic/ckeditor.js"></script>
    <script>
        ClassicEditor
            .create(document.querySelector('#Description'))
            .catch(error => {
                console.error(error);
            });
    </script>
   

</body>

</html>

